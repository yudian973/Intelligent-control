    77            C        ;              X(k|k)   = X(k|k-1)+Kg(k)(Z(k)-H X(k|k-1))
    78            C        ;              Kg(k)    = P(k|k-1)H/(HP(k|k-1)H+R)
    79            C        ;              P(k|k)   = (I-Kg(k)H)P(k|k-1)
    80            C        ;====================================================================
