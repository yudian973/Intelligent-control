   105 01D0B 1C01 C        	RETL                @1
   106 01D0C 1C00 C        	RETL                @0
   107 01D0D 1C00 C        	RETL                @0
   108 01D0E 1C01 C        	RETL                @1
   109            C        ;                          | h11,h12 |
   110            C        ; H[4]={h11,h12,h31,h32} = |         |
   111            C        ;                          | h31,h32 |
   112            C        
   113            C        ;------------------------------------------------------
   114            C        
   115 01D0F      C        KalmanFilter_2:
   116 01D0F 0000 C        	NOP
   117 01D10 0000 C        	NOP
   118            C        
   119            C        
   120            C        
   121            C        
   122 01D11 0012 C        	RET
   123            C        
   124            C        ;====================================================================
   125            C        ; name:        Kalman Filter arithmetic three
   126            C        ; input:
   127            C        ; output:
   128            C        ; description: a three-dimensional space value(3D)
   129            C        ;              X(k|k-1) = AX(k-1|k-1)+BU(k)
   130            C        ;              P(k|k-1) = AP(k-1|k-1)A'+Q
   131            C        ;              X(k|k)   = X(k|k-1)+Kg(k)(Z(k)-H X(k|k-1))
   132            C        ;              Kg(k)    = P(k|k-1)H/(HP(k|k-1)H+R)
