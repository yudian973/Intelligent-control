    32            C        ; name:        Kalman Filter arithmetic one
    33            C        ; input:
    34            C        ; output:
    35            C        ; description: a single value input plane(Point)
    36            C        ;              X(k|k-1) = AX(k-1|k-1)+BU(k)+W(k)
    37            C        ;              P(k|k-1) = AP(k-1|k-1)A'+Q
    38            C        ;              X(k|k)   = X(k|k-1)+Kg(k)(Z(k)-H X(k|k-1))
    39            C        ;              Kg(k)    = P(k|k-1)H/(HP(k|k-1)H+R)
    40            C        ;              P(k|k)   = (I-Kg(k)H)P(k|k-1)
    41            C        ;====================================================================
    42            C        
    43            C        ;------------------------------------------------------
    44       0001 C        KF1_A                   EQU    @1
    45       0001 C        KF1_B                   EQU    @1
    46       0001 C        KF1_H                   EQU    @1
    47            C        ;------------------------------------------------------
    48            C        
    49 01C00      C        KalmanFilter_1:
    50            C        ;   X(k|k-1) = AX(k-1|k-1)+BU(k)+W(k) |
    51            C        ;   A=1                               |
    52            C        ;   B=1                               | => X(k|k-1) = X(k-1|k-1)
    53            C        ;   U(k)=0                            |
    54            C        ;   W(k)=0                            |
    55 01C00 1E05 C        	BANK                5
    56            C        	;MOV                 A,X_kR1_kR1
    57            C        	;MOV                 X_k_kR1,A
    58            C        
    59            C        ;   P(k|k-1) = AP(k-1|k-1)A'+Q        |
    60            C        ;   A=1                               | => P(k|k-1) = P(k-1|k-1)+Q
    61            C        ;   Q=cov(W(k),W(k))=                 |
    62            C        
    63            C        
    64            C        
    65            C        
    66 01C01 0012 C        	RET
    67            C        
    68            C        
    69            C        
    70            C        ;====================================================================
    71            C        ; name:        Kalman Filter arithmetic two
    72            C        ; input:
    73            C        ; output:
