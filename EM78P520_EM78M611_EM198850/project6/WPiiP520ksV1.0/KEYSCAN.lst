   530            C        	JMP     $-13           ; CLEAR_RAM_LOOP_P510
   531            C        	CLR     R4
   532            C        	CLR     BSR
   533            C        	BS      R4,6
   534            C        
   535            C        ELSEIFDEF EM78M611.H
   536            C        	CLR     RC
   537            C        	CLR     RD
   538            C        	CLR     0X10
   539            C        	MOV     A,@0X2F
   540            C        	MOV     0X10,A
   541            C        
   542            C        	MOV     A,@0X11
   543            C        	MOV     RSR,A
   544            C        ;CLRLOOPU1:
   545            C        	CLR     R0
   546            C        	INC     RSR
   547            C        	DJZ     0X10
   548            C        	JMP     $-3        ;CLRLOOPU1
   549            C            CLR     R0
   550            C        	MOV     A,@0X20
   551            C        	MOV     0X10,A
   552            C        	MOV     A,@0X60
   553            C        	MOV     RSR,A
   554            C        
   555            C        ;CLRLOOPU2:
   556            C        	CLR     R0
   557            C        	INC     RSR
   558            C        	DJZ     0X10
   559            C        	JMP     $-3             ;CLRLOOPU2
   560            C        
   561            C        	CLR     R0
   562            C        	MOV     A,@0X20
   563            C        	MOV     0X10,A
   564            C        	MOV     A,@0XA0
   565            C        	MOV     RSR,A
   566            C        
   567            C        ;CLRLOOPU3:
   568            C        	CLR     R0
   569            C        	INC     RSR
   570            C        	DJZ     0X10
   571            C        	JMP     $-3            ;CLRLOOPU3
   572            C        	CLR     R0
   573            C        
   574            C        	MOV     A,@0X11
   575            C        	MOV     RD,A
   576            C        	CLR     RE
   577            C        	MOV     A,@1
   578            C        	MOV     RD,A
   579            C        	MOV     A,@0X00
   580            C        	MOV     RE,A
   581            C        	MOV     RE,A
   582            C        	MOV     RE,A
   583            C        	MOV     RE,A
   584            C        	MOV     RE,A
   585            C        	MOV     RE,A
   586            C        	MOV     RE,A
   587            C        	MOV     RE,A
   588            C        	BS      RC,5
   589            C        
   590            C        
   591            C        ELSEIFDEF EM78P447.H
   592            C         	MOV     A,@0x10
   593            C        	MOV     R4,A
   594            C        ;CLEAR_RAM_LOOP_P447:
   595            C        	CLR     R0
   596            C        	INC     R4
